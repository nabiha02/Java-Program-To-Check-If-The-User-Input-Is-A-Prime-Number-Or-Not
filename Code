import java.util.Scanner;

public class lab2_task2{

    public static void main(String[] args) {

        Scanner input = new Scanner(System.in);

        System.out.println("Enter your num: ");
        int num = input.nextInt();
        int count =0;

        for(int i=2; i<num; i++){
            if(num%i==0){
                count++;
                break;
            }
